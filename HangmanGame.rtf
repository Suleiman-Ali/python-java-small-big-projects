    \f0\ltrch\lang2057 display\f1\rtlch\lang1025  += "_"\par
\par
\f0\ltrch\lang2057 while not game_is_finished\f1\rtlch\lang1025 :\par
    \f0\ltrch\lang2057 guess = input("Guess a letter: ").lower\f1\rtlch\lang1025 ()\par
