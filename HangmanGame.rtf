        \f0\ltrch\lang2057 print(f"You've already guessed \{guess\}")\f1\rtlch\lang1025\par
\par
    \f0\ltrch\lang2057 for position in range(word_length)\f1\rtlch\lang1025 :\par
        \f0\ltrch\lang2057 letter = chosen_word[position]\f1\rtlch\lang1025\par
        \f0\ltrch\lang2057 if letter == guess\f1\rtlch\lang1025 :\par
            \f0\ltrch\lang2057 display[position] = letter\f1\rtlch\lang1025\par
    \f0\ltrch\lang2057 print(f"\{' '.join(display)\}")\f1\rtlch\lang1025\par
\par
    \f0\ltrch\lang2057 if guess not in chosen_word\f1\rtlch\lang1025 :\par
        \f0\ltrch\lang2057 print(f"You guessed \{guess\}, that's not in the word. You lose a life.")\f1\rtlch\lang1025\par
        \f0\ltrch\lang2057 lives -= 1\f1\rtlch\lang1025\par
        \f0\ltrch\lang2057 if lives == 0\f1\rtlch\lang1025 :\par
            \f0\ltrch\lang2057 game_is_finished = True\f1\rtlch\lang1025\par
            \f0\ltrch\lang2057 print("The Man is dead because of you !\\nGame over.")\f1\rtlch\lang1025\par
            \f0\ltrch\lang2057 print(f"The word was -> \{chosen_word\}")\f1\rtlch\lang1025\par
\par
    \f0\ltrch\lang2057 if not "_" in display\f1\rtlch\lang1025 :\par
        \f0\ltrch\lang2057 game_is_finished = True\f1\rtlch\lang1025\par
        \f0\ltrch\lang2057 print("You win.")\f1\rtlch\lang1025\par
\par
